<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      .list{ margin-left: 275px; }
      table {
            width: 100%;
            border-collapse: collapse;
        }

        th,
        td {
            padding: 10px;
            border-bottom: 1px solid #ddd;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }
    </style>
  </head>
  <body>
    <div id="adminNavbar"></div>
    <div class="list">
      <h2>Feedback List</h2>
      <table>
        <thead>
          <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Message</th>
          </tr>
        </thead>
        <tbody id="feedback-table-body">
          <!-- rows will be add dynamically -->
        </tbody>
      </table>
    </div>
    <script src="/admin_script.js"></script>
    <script>
      fetch('/adminNavbar') .then(response => response.text()) .then(data =>
      document.getElementById('adminNavbar').innerHTML = data);
    </script>
  </body>
</html>